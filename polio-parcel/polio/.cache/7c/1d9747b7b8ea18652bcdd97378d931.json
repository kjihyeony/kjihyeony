100vw;\n$vw-fontsize-sm: 10 / $small * 100vw;\n$vw-fontsize-xsm: 10 / $xsmall * 100vw;\n","css/mixins/_breakpoint.scss":"// 브레이크포인트 mixin\n\n@mixin respond-to($breakpoint) {\n  @if $breakpoint == \"xlarge\" {\n    @media screen and (max-width: map-get($breakpoints, $breakpoint)) and (min-width:map-get($breakpoints, 'medium') + 1px) {\n      @content;\n    }\n  }\n  @else if $breakpoint == \"medium\" {\n      @media screen and (max-width: map-get($breakpoints, $breakpoint)) and (min-width:1px) {\n          @content;\n      }\n  }\n  @else if $breakpoint == \"small\" {\n      @media screen and (max-width: map-get($breakpoints, $breakpoint)) and (min-width:1px) {\n          @content;\n      }\n  }\n  @else if $breakpoint == \"xsmall\" {\n      @media screen and (max-width: map-get($breakpoints, $breakpoint)) and (min-width:1px) {\n          @content;\n      }\n  }\n}\n","css/_main.scss":"/* ================= main ================= */\n.main {\n  display: flex;\n  height: 100vh;\n}\n/* ================= main - left ================= */\n.main {\n  &-wrapper {\n    position: relative;\n    width: calc( 100% - 8rem);\n  }\n  .left {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    height: 100%;\n    z-index: 2;\n\n    &-line {\n      position: absolute;\n      top: 0;\n      left: 0;\n      right: 0;\n      bottom: 0;\n      z-index: 1;\n    }\n  }\n\n  .h9figureVideo {\n    position: relative;\n    width: 70rem;\n    z-index: 2;\n    opacity: 0.5;\n\n    video{\n      width: 100%;\n    }\n  }\n\n  .h9figure{\n    position: absolute;\n    z-index: 1;\n\n    img {\n      display: block;\n    }\n  }\n}\n\n/* ================= main - right ================= */\n.main {\n  position: relative;\n  z-index: 1;\n  .right {\n    display: flex;\n    align-items: center;\n    width: 8rem;\n    padding-top: 4.5rem;\n    border-left: 1px solid $primary-blue;\n    writing-mode: tb-rl;\n\n    a {\n      font-size: 2rem;\n    }\n  }\n}\n\n/* ================= main - cursor ================= */\n#cursor {\n  position: fixed;\n  z-index: 100;\n  top: -1.5rem;\n  left: -1.5rem;\n  width: 1rem;\n  height: 1rem;\n  transform: translate(0);\n  transform-origin: 50% 50%;\n  pointer-events: none;\n\n  .sizer {\n    position: absolute;\n    z-index: 2;\n    top: -1rem;\n    left: -1rem;\n    width: 4.7rem;\n    height: 4.7rem;\n    background: #f63845;\n    border-radius: 50%;\n    // transition: background .5s cubic-bezier(.14,.99,.43,.99),-webkit-transform .5s cubic-bezier(.14,.99,.43,.99);\n    transition: transform .5s cubic-bezier(.14,.99,.43,.99),background .5s cubic-bezier(.14,.99,.43,.99);\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    transform-origin: 50% 50%;\n    transform: scale(0);\n\n    &.active {\n      transform: scale(4);\n    }\n  }\n\n}\n\n/* ================= main - copyright ================= */\n.copy {\n  position: fixed;\n  bottom: 2rem;\n  left: 3.4rem;\n  font-size: 1.4rem;\n}"},"lineCount":null}},"error":null,"hash":"06f78c5668f57c9df2a16ef11c961f23","cacheData":{"env":{}}}